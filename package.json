{
  "name": "saldutdools-fullstack-challenge",
  "version": "2.1.0",
  "private": true,
  "description": "Saludtools Fullstack Challenge",
  "author": "jlondono@saludtools.com",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "e2e": "node test/e2e/runner.js",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve --open",
    "lint-fix": "vue-cli-service lint --fix",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "/node_modules/(?!.*\\.vue)",
      "/node_modules/(?!axios)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json",
        "diagnostics": false
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "@vue/test-utils": "2.2.10",
    "axios": "1.2.3",
    "babel-core": "7.0.0-bridge.0",
    "bootstrap": "4.6.0",
    "bootstrap-vue": "2.23.1",
    "cache-loader": "4.1.0",
    "jquery": "3.6.3",
    "register-service-worker": "1.7.2",
    "v-tooltip": "2.0.0-rc.33",
    "vue": "2.7.14",
    "vue-router": "3.0.2",
    "vue-toast": "3.1.0",
    "vue-toast-notification": "0.6.3",
    "vue-toasted": "1.1.28",
    "vuelidate": "0.7.7"
  },
  "devDependencies": {
    "@babel/core": "7.12.0",
    "@babel/preset-env": "7.12.1",
    "@babel/register": "7.0.0",
    "@types/jest": "29.4.0",
    "@vue/cli-plugin-babel": "3.12.1",
    "@vue/cli-plugin-eslint": "3.12.1",
    "@vue/cli-plugin-pwa": "3.12.1",
    "@vue/cli-plugin-unit-jest": "4.5.13",
    "@vue/cli-service": "3.12.1",
    "babel-jest": "24.9.0",
    "jest": "27.4.7",
    "sass": "1.56.2",
    "sass-loader": "10.1.1",
    "ts-jest": "27.0.3",
    "typescript": "4.9.5",
    "vue-jest": "3.0.7",
    "vue-template-compiler": "2.7.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}